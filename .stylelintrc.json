{
  "extends": ["stylelint-config-standard", "stylelint-config-prettier"],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["extends", "tailwind", "apply"]
      }
    ],
    "block-no-empty": null,
    "unit-whitelist": ["em", "rem", "s", "%", "deg", "px"]
  }
}
