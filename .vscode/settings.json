{
  "http.proxySupport": "off",
  "eslint.probe": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "css.validate": false,
  "scss.validate": false,
  "stylelint.enable": true,
  "vsicons.associations.files": [
    {
      "icon": "webpack",
      "extensions": ["main.config.js", "renderer.config.js", "main.plugins.js", "renderer.plugins.js", "rules.js"],
      "filename": true,
      "format": "svg"
    },
    {
      "icon": "tailwind",
      "extensions": ["tailwind.theme.config.js"],
      "filename": true,
      "format": "svg"
    }
  ]
}
