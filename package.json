{
  "name": "mdc-electron",
  "description": "A visual tool for developing morph modules.",
  "version": "0.0.1",
  "author": "Sam Maister <sam.maister@bbc.co.uk>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goosewobbler/mdc-electron.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "tsc --outDir lib --rootDir src",
    "buildServer": "tsc -p tsconfig-source-only.json --outDir lib --rootDir src",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run clean && npm run build && electron ./lib/main.js",
    "jest": "jest --watch"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "graceful-fs": "^4.2.0",
    "lodash": "^4.17.15",
    "ls": "^0.2.1",
    "morph-cli": "^11.6.1",
    "ncp": "^2.0.0",
    "node-object-hash": "^1.4.2",
    "node-watch": "^0.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.9",
    "react-list": "^0.8.11",
    "react-redux": "^7.1.0",
    "redux-thunk": "^2.3.0",
    "semver": "^6.3.0",
    "socket.io": "^2.2.0",
    "tar": "^4.4.10"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/fs-extra": "^8.0.0",
    "@types/graceful-fs": "^4.1.3",
    "@types/lodash": "^4.14.136",
    "@types/ncp": "^2.0.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-list": "^0.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/request": "^2.48.2",
    "@types/semver": "^6.0.1",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/tar": "^4.0.3",
    "electron": "^5.0.8",
    "eslint": "^6.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-airbnb-base": "^0.3.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3"
  }
}
