    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "global": "^4.4.0",
    "graceful-fs": "^4.2.3",
    "lodash": "^4.17.15",
    "ls": "^0.2.1",
    "morph-cli": "^12.0.4",
    "ncp": "^2.0.0",
