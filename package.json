  },
  "resolutions": {
    "postcss": "^8.0.0"
  },
  "browserslist": [
    "electron >= 15",
    "node >= 14"
  ],
  "main": "./dist/main.prod.js",
  "build": {
    "asar": true,
    "appId": "com.dj_helper.demo",
    "copyright": "goosewobbler",
    "productName": "dj_helper",
    "files": [
      "dist/**/*"
    ]
  }
}
