{
  "name": "dj-helper",
  "description": "A tool for constructing DJ sets",
  "version": "0.0.1",
  "author": "Sam Maister <goosewobbler@protonmail.com>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goosewobbler/dj-helper.git"
  },
  "main": "index.js",
  "scripts": {
    "init": "sh ./scripts/setup.sh && pnpm install",
    "clean": "rm -rf ./dist",
    "clean:all": "rm -rf pnpm-lock.yaml ./node_modules ./dist",
    "dev": "pnpm clean && pnpm dev:build-main && cross-env pnpm dev:start-renderer",
    "dev:upgrade-extensions": "UPGRADE_EXTENSIONS=true pnpm dev",
    "dist": "pnpm build && electron-builder",
    "build": "pnpm clean && concurrently 'pnpm build:main' 'pnpm build:renderer'",
    "build:main": "cross-env NODE_ENV=production webpack --config ./webpack/main.config.js --color",
    "build:renderer": "cross-env NODE_ENV=production webpack --config ./webpack/renderer.config.js --color",
    "start": "cross-env NODE_ENV=production electron ./dist/main.prod.js",
    "dev:build-main": "cross-env NODE_ENV=development webpack --config ./webpack/main.config.js --color",
    "dev:start-main": "cross-env NODE_ENV=development electron -r @babel/register ./dist/main.dev.js",
    "dev:start-renderer": "cross-env NODE_ENV=development webpack serve --hot --config ./webpack/renderer.config.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "concurrently 'pnpm lint:code' 'pnpm lint:styles'",
    "lint:code": "eslint '**/*.[j|t]s{,x}'",
    "lint:code:fix": "pnpm lint:code --fix",
    "lint:styles": "stylelint --ignore-path .eslintignore '**/*.pcss'",
    "lint:styles:fix": "pnpm --silent lint-styles --fix; exit 0",
    "test": "pnpm test:setup && pnpm test:coverage && pnpm format && pnpm lint",
    "test:setup": "mkdir -p ./dist",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:watch-coverage": "jest --watch --coverage",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@mckayla/electron-redux": "^3.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@redux-offline/redux-offline": "^2.6.0",
    "@reduxjs/toolkit": "^1.6.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/app-root-path": "^1.2.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "@types/request": "^2.48.5",
    "@types/source-map-support": "^0.5.2",
    "@types/stylelint": "^13.13.1",
    "@types/tailwindcss": "^2.2.1",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "app-root-path": "^3.0.0",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "classnames": "^2.2.6",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "cssnano": "^5.0.6",
    "electron": "^13.1.5",
    "electron-builder": "^22.11.7",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-store": "^8.0.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "html-webpack-harddisk-plugin": "^2.0.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "mini-css-extract-plugin": "^2.0.0",
    "node-loader": "^2.0.0",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-refresh": "^0.10.0",
    "redux-devtools-extension": "^2.13.9",
    "source-map-loader": "^3.0.0",
    "source-map-support": "^0.5.19",
    "spectron": "^15.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "tailwindcss": "^2.2.4",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.42.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "resolutions": {
    "postcss": "^8.0.0"
  },
  "browserslist": [
    "electron >= 13",
    "node >= 14"
  ]
}
