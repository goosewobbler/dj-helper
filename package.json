{
  "name": "mdc-electron",
  "description": "A visual tool for developing morph modules.",
  "version": "4.0.0",
  "author": "Sam Maister <sam.maister@bbc.co.uk>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goosewobbler/mdc-electron.git"
  },
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean:all": "rm -rf yarn.lock ./node_modules ./dist",
    "clean": "rm -rf ./dist",
    "dev-server": "webpack-dev-server --hot --mode development",
    "dev": "cross-env START_HOT=1 node -r ./scripts/check-port.js && cross-env START_HOT=1 yarn dev:start-renderer",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "build": "concurrently \"yarn build:main\" \"yarn build:renderer\"",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack.main.config.js --colors",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.config.js --colors",
    "start": "cross-env NODE_ENV=production electron ./dist/main.prod.js",
    "dev:start-main": "cross-env START_HOT=1 NODE_ENV=development electron src/main/index.ts",
    "dev:start-renderer": "cross-env NODE_ENV=development webpack-dev-server --config webpack.renderer.config.js",
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:js": "eslint --config '.eslintrc.js.json' '**/*.js'",
    "lint:ts": "eslint --config '.eslintrc.ts.json' '**/*.ts{,x}'"
  },
